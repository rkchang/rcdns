    "version": "0.2.0",
    "configurations": [
        {
            "name": "rcdns",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build_debugger/rcdns",
            "args": [
                "44532",
                "8.8.8.8"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
